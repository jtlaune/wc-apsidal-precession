@misc{nelsonGasAccretionJupiter2022,
  title = {Gas Accretion onto {{Jupiter}} Mass Planets in Discs with Laminar Accretion Flows},
  author = {Nelson, R. P. and Lega, E. and Morbidelli, A.},
  year = {2022},
  month = dec,
  journal = {arXiv e-prints},
  doi = {10.48550/arXiv.2212.08012},
  abstract = {(Abridged) Studies have shown that a Jovian mass planet embedded in a viscous protoplanetary disc (PPD) can accrete gas efficiently through the gap and doubles its mass in \$\textbackslash sim 0.1\$ Myr. The planet also migrates inwards on a timescale of \$\textbackslash sim 0.1\$ Myr. These timescales are short compared to PPD lifetimes, and raise questions about the origins of cold giant exoplanets. However, PPDs are unlikely to be globally turbulent, and instead they may launch magnetised winds such that accretion towards the star occurs in laminar accretion flows located in narrow layers near the surfaces of the disc. The aim of this study is to examine the rate at which gas accretes onto Jovian mass planets that are embedded in layered PPDs. We use 3D hydrodynamical simulations of planets embedded in PPDs, in which a constant radial mass flux towards the star of \$\{\textbackslash dot m\} = 10\^\{-8\}\$ M\$\_\{\textbackslash odot\}\$ yr\$\^\{-1\}\$ is sustained. We consider a classical viscous alpha model, and also models in which an external torque is applied in narrow surface layers to mimic the effects of a magnetised wind. The accreting layers are parameterised by their column densities \$\textbackslash Sigma\_\{\textbackslash rm A\}\$, and we consider values in the range 0.1 to 10 g cm\$\^\{-2\}\$. The viscous model gives results in agreement with previous studies. We find the accretion rate onto the planet in the layered models crucially depends on the planet's ability to block the wind-induced mass flow. For \$\textbackslash Sigma\_\{\textbackslash rm A\}=10\$ g cm\$\^\{-2\}\$, the planet torque can block the mass flow through the disc, accretion onto the planet is slow, and a mass doubling time of 10 Myr is obtained. For \$\textbackslash Sigma\_\{\textbackslash rm A\}=0.1\$ g cm\$\^\{-2\}\$, accretion is fast and the mass doubling time is 0.2 Myr. Although the radial mass flow through the layered disc models is always \$10\^\{-8\}\$ M\$\_\{\textbackslash odot\}\$ yr\$\^\{-1\}\$, adopting different values of \$\textbackslash Sigma\_\{\textbackslash rm A\}\$ leads to very different gas accretion rates onto gas giant planets.},
  keywords = {Astrophysics - Earth and Planetary Astrophysics},
  annotation = {ADS Bibcode: 2022arXiv221208012N},
  file = {/Users/jtlaune/Zotero/storage/XLXC9UJA/Nelson et al. - 2022 - Gas accretion onto Jupiter mass planets in discs w.pdf}
}

@article{nesvorny_koi-142_2013,
  title = {{{KOI-142}}, {{THE KING OF TRANSIT VARIATIONS}}, {{IS A PAIR OF PLANETS NEAR THE}} 2:1 {{RESONANCE}}},
  shorttitle = {{{KOI-142}}, {{THE KING OF TRANSIT VARIATIONS}}, {{IS A PAIR OF PLANETS NEAR THE}} 2},
  author = {Nesvorn{\'y}, David and Kipping, David and Terrell, Dirk and Hartman, Joel and Bakos, G{\'a}sp{\'a}r {\'A} and Buchhave, Lars A.},
  year = {2013},
  month = oct,
  journal = {The Astrophysical Journal},
  volume = {777},
  number = {1},
  pages = {3},
  issn = {0004-637X},
  doi = {10.1088/0004-637X/777/1/3},
  abstract = {The transit timing variations (TTVs) can be used as a diagnostic of gravitational interactions between planets in a multi-planet system. Many Kepler Objects of Interest (KOIs) exhibit significant TTVs, but KOI-142.01 stands out among them with an unrivaled {$\simeq$}12 hr TTV amplitude. Here we report a thorough analysis of KOI-142.01's transits. We discover periodic transit duration variations (TDVs) of KOI-142.01 that are nearly in phase with the observed TTVs. We show that KOI-142.01's TTVs and TDVs uniquely detect a non-transiting companion with a mass {$\simeq$}0.63 that of Jupiter (KOI-142c). KOI-142.01's mass inferred from the transit variations is consistent with the measured transit depth, suggesting a Neptune-class planet (KOI-142b). The orbital period ratio Pc /Pb = 2.03 indicates that the two planets are just wide of the 2:1 resonance. The present dynamics of this system, characterized here in detail, can be used to test various formation theories that have been proposed to explain the near-resonant pairs of exoplanets.},
  langid = {english}
}

